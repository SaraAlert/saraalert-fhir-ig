      </reference>
      <name value="Sara Alert Observation Profile"/>
      <description value="The Sara Alert Observation profile represents a monitoree lab result."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:resource"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/sara-alert-patient"/>
      </reference>
      <name value="Sara Alert Patient Profile"/>
      <description value="Sara Alert outputs additional extensions on Patient resources"/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
