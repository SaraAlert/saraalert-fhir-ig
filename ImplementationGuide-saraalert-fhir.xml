      </reference>
      <name value="Sara Alert QuestionnaireResponse Profile"/>
      <description value="The Sara Alert QuestionnaireResponse profile represents a monitoree daily report."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/additional-planned-travel-notes"/>
      </reference>
      <name value="Additional Planned Travel Notes"/>
      <description value="Represents any additional notes about the monitoree&#x2019;s planned travel. This can include details about the nature of the trip, planned social interaction, additional destinations, etc."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/additional-planned-travel-start-date"/>
      </reference>
      <name value="Additional Planned Travel Start Date"/>
      <description value="Represents the date that the monitoree is planning to begin their additional travel."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/assigned-user"/>
      </reference>
      <name value="Assigned User"/>
      <description value="Represents the user or group within a jurisdiction responsible for monitoring a monitoree."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/date-of-arrival"/>
      </reference>
      <name value="Date of Arrival"/>
      <description value="Represents when the monitoree entered the United States after travel."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/date-of-departure"/>
      </reference>
      <name value="Date of Departure"/>
      <description value="Represents the date that the monitoree traveled from the Port of Origin to the United States."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/exposure-notes"/>
      </reference>
      <name value="Exposure Notes"/>
      <description value="Represents any additional notes about the monitoree&#x2019;s exposure history or case information history. This can include details about the interaction with a known case, high risk activities, common exposure, etc."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/flight-or-vessel-carrier"/>
      </reference>
      <name value="Flight or Vessel Carrier"/>
      <description value="Represents the carrier, operating company, or provider of the flight or vessel."/>
      <exampleBoolean value="false"/>
    </resource>
    <resource>
      <extension url="http://hl7.org/fhir/tools/StructureDefinition/resource-information">
        <valueString value="StructureDefinition:extension"/>
      </extension>
      <reference>
        <reference value="StructureDefinition/flight-or-vessel-number"/>
      </reference>
      <name value="Flight or Vessel Number"/>
